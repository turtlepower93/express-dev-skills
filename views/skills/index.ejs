<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>Skills</title>
</head>
<body>
    <a href="/">Home</a>
    <h1>Skills</h1>
    <ul>
        <% skills.forEach(function(t) { %>
          <li>
            <form action="/skills/<%= t.id %>?_method=DELETE"
              class="delete-form" method="POST">
              <button type="submit">X</button>
            </form>
            <a href="/skills/<%= t.id %>"><%= t.skill %></a>
          </li>
        <% }); %>
      </ul>
      <a href="skills/new">New Skill</a>
</body>
</html>